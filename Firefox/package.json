  "version": "1.2.3",

    "permissions": {
        "cross-domain-content": ["http://www.reddit.com/", "https://pay.reddit.com/"]
    },

    "preferences": [{
        "name": "commentVoteThreshold",
        "title": "Don't show me comments that is liked by less than this many percent of people",
        "description": "",
        "type": "string",
        "value": "10"
    },
    {
        "name": "featherDescriptionPlacement",
        "title": "Collapse comment box into description in YouTube Feather",
        "description": "",
        "type": "bool",
        "value": false
    },
    {
        "name": "disablePostHeader",
        "title": "Hide the post header",
        "description": "",
        "type": "bool",
        "value": false
    },
    {
        "name": "disableTabs",
        "title": "Hide the tab bar",
        "description": "",
        "type": "bool",
        "value": false
    },
    {
        "name": "minimiseCommentBox",
        "title": "Minimise the comment box",
        "description": "",
        "type": "bool",
        "value": false
    },
    {
        "name": "dontShowGplus",
        "title": "Do not display the Google Plus comment section when no results are found.",
        "description": "",
        "type": "bool",
        "value": false
    }]
}
